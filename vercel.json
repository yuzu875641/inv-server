{
  "version": 2,
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/video/(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/live/(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/proxy-hls",
      "dest": "/api/yt.js"
    },
    {
      "src": "/proxy-hls-segment",
      "dest": "/api/yt.js"
    },
    {
      "src": "/proxy-dash",
      "dest": "/api/yt.js"
    },
    {
      "src": "/proxy-stream",
      "dest": "/api/yt.js"
    }
  ]
}
