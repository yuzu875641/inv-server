{
  "version": 2,
  "builds": [
    {
      "src": "api/yt.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/live/highest-quality-stream/(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/live/(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/video/(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/proxy-stream(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/proxy-hls(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/proxy-hls-segment(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/proxy-hls-segments(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/proxy-dash(.*)",
      "dest": "/api/yt.js"
    },
    {
      "src": "/proxy-dash-segment(.*)",
      "dest": "/api/yt.js"
    }
  ]
}
